		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		        
		    }
		    System.out.print(" ");
		    //for m
		    for(int j=0;j<=n-1;j++){
		       if(j==0 || j==n-1 || ((i+j==n-1 || i==j) && i<=n/2) ){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		        
		    }
		    System.out.print(" ");
