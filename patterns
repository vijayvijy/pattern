import java.util.*;
 class P1
{
	public static void main(String[] args) {
	    Scanner scan= new Scanner(System.in);
	    System.out.println("enter a number of rows");
	    int n=scan.nextInt();
	    System.out.println();
		for(int i=0;i<=n-1;i++){//for j
		    for(int j=0;j<=n-1;j++){
		       if(i==0 || j==n/2 || i-j==n/2){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		        
		    }
		    
		    System.out.print("  ");
		    //for a
		    for(int j=0;j<=n-1;j++){
		       if((j==0 && i!=0) || (j==n-1 && i!=0) || i==n/2 || (i==0 && j!=0 && j!=n-1) ){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		        
		    }
		    System.out.print("  ");
		    //for v
		    for(int j=0;j<=n-1;j++){
		       if(((j==n-1 || j==0)&& i<=n/2) || j==i-n/2 || i+j==n+2 && i>=n/2){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		        
		    }
		    System.out.print("  ");
		    //for a
		    for(int j=0;j<=n-1;j++){
		       if((j==0 && i!=0) || (j==n-1 && i!=0) || i==n/2 || (i==0 && j!=0 && j!=n-1) ){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		        
		    }
		    System.out.print("         ");
		    //for i
		    for(int j=0;j<=n-1;j++){
		       if((i==0 || i==n-1 || j==n/2 )){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		        
		    }
		     System.out.print(" ");
		    //for s
		    for(int j=0;j<=n-1;j++){
		       if(( i==0 && j!=0) || (i==n/2 && j!=0 && j!=n-1) || (i==n-1 && j!=n-1) || ( j==n-1 && i>n/2 && i<n-1) || (j==0 && i!=0 && i<n/2)){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		        
		    }
		     System.out.print("      ");
