		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		        
		    }
		    	System.out.print(" ");
		    	// for g
		    for(int j=0;j<=n-1;j++){
		       if(i==0 || j==0 || (j==n-1 && i!=n/2-1 ) || (i==n/2 && j>=n/2) || (i==n-1 && j<=n/2) || (j==n/2 && i>=n/2)){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		         
		    }
		    System.out.println(); 
		}
		System.out.println(" "); 
		System.out.println(); 
		for(int i=0;i<=n-1;i++){
		    // for p
		    for(int j=0;j<=n-1;j++){
		       if(j==0 || ((i==0 || i==n/2) && j!=n-1) || (j==n-1 && i>0 && i<n/2)){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		         
		    }
		    System.out.print(" ");
		    // for r
		    for(int j=0;j<=n-1;j++){
		       if(j==0 || ((i==0 || i== n/2) && j<=n/2) || (j==n/2+1 && i!=0 && i<n/2) || i-j==n/2){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		         
		    }
		    System.out.print(" ");
		    // for o
		    for(int j=0;j<=n-1;j++){
		       if(((i==0 ||i==n-1) && j!=0 && j!=n-1) || ((j==0 || j==n-1) && i!=0 && i!=n-1)){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		         
		    }
		    System.out.print(" ");
		    // for u
		    for(int j=0;j<=n-1;j++){
		       if(((j==0 || j==n-1) && i!=n-1) || (i==n-1 && j!=0 && j!=n-1)){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		         
		    }
		    System.out.print(" ");
