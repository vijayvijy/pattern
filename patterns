		       if(i==0 || j==0 || (j==n-1 && i!=n/2-1 ) || (i==n/2 && j>=n/2) || (i==n-1 && j<=n/2) || (j==n/2 && i>=n/2)){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		         
		    }
		    System.out.print(" ");
		    	// for q
		    for(int j=0;j<=n-1;j++){
		       if(((i==0 || i==n/2+1) && j!=0 && j<=n/2) || ((j==0 || j==n/2+1) && i!=0 && i<=n/2) || (i==j && i>=n/2) ){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		         
