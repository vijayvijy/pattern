		    for(int j=0;j<=n-1;j++){
		       if((i==0 || i==n-1 || j==n/2 )){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		        
		    }
		    System.out.print(" ");
