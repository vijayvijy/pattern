		    for(int j=0;j<=n-1;j++){
		       if((j==0  || (i==0 || i==n-1) && (j!=n-1)) || (j==n-1 && i!=0 && i!=n-1) ){
		        System.out.print("*"); 
		       }
		       else{
		          System.out.print(" ");  
		       }
		         
		    }
